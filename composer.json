{
    "name": "kanata-php/socket-conveyor",
    "description": "A WebSocket/Socket message Router",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "league/pipeline": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1",
        "phpunit/phpunit": "^9.2",
        "mockery/mockery": "^1.4",
        "codedungeon/phpunit-result-printer": "^0.30.0"
    },
    "autoload": {
        "psr-4": {
            "Conveyor\\": "src/",
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --testsuite All"
        ]
    },
    "bin": [
        "start-ws-server"
    ]
}
